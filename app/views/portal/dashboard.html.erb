<h1>Portal#dashboard</h1>
<p>Find me in app/views/portal/dashboard.html.erb</p>
<%= link_to "Log out", destroy_user_session_path, :method => :delete %>

<h2>Services</h2>

<table class='table table-striped table-bordered'>
    
  <tr>
    <th>Service</th>
    <th>License Key</th>
  </tr>
    
    <% @user.licenses.each do |license| %>
        <tr>
          <td><%= license.product.name %></td>
          <td><%= license.key %></td>
        </tr>
    <% end %>

</table>